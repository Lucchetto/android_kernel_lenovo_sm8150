obj-$(CONFIG_TOUCHSCREEN_FTS_LENOVO)	+=  focaltech_flash.o
obj-$(CONFIG_TOUCHSCREEN_FTS_LENOVO)	+=  focaltech_gesture.o
obj-$(CONFIG_TOUCHSCREEN_FTS_LENOVO)	+=  focaltech_esdcheck.o
obj-$(CONFIG_TOUCHSCREEN_FTS_LENOVO)   +=  focaltech_i2c.o
obj-$(CONFIG_TOUCHSCREEN_FTS_LENOVO)   +=  focaltech_point_report_check.o
obj-$(CONFIG_TOUCHSCREEN_FTS_LENOVO)   +=  focaltech_flash/

